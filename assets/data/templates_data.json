  "autoRegulationRules": {
    "ratings": {
      "1": {
        "label": "Too Easy",
        "action": "Jump forward +2 reps or next step"
      },
      "2": {
        "label": "Easy",
        "action": "Progress faster (+1 rep next session)"
      },
      "3": {
        "label": "Just Right",
        "action": "Continue as planned"
      },
      "4": {
        "label": "Hard",
        "action": "Repeat current week"
      },
      "5": {
        "label": "Too Hard",
        "action": "Step back -1 rep or previous week"
      }
    },
    "failedSetThresholds": {
      "1": "Continue, note for next session",
      "2": "Reduce reps by 1 next session",
      "3": "Return to previous week's protocol"
    }
  }
